# The train/test net protocol buffer definition
train_net: "./model/proto_train.prototxt"

max_iter: 40000
display: 10000
snapshot: 2500
snapshot_prefix: "./snapshots/ACT-X/release"

# The base learning rate, momentum and the weight decay of the network.
solver_type: ADAM
base_lr: 0.01
momentum: 0.9
momentum2: 0.999
weight_decay: 0.000
lr_policy: "fixed"
